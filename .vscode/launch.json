  "version": "0.2.0",
  "configurations": [
      {
          "name": "Physio Calc (dev mode)",
          "request": "launch",
          "type": "dart",
          "program": "lib/main.dart",
          "args": [
              "--flavor",
              "dev",
              "--target",
              "lib/main.dart"
          ]
      },
      {
          "name": "Physio Calc (demo mode)",
          "request": "launch",
          "type": "dart",
          "program": "lib/main_demo.dart",
          "args": [
              "--flavor",
              "demo",
              "--target",
              "lib/main_demo.dart"
          ]
      },
      {
          "name": "Physio Calc (release mode)",
          "request": "launch",
          "type": "dart",
          "flutterMode": "release",
          "args": [
              "--flavor",
              "prod",
              "--target",
              "lib/main_prod.dart"
          ]
      }
  ]
